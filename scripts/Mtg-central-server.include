#
# ProduceBE_Booster() - Produce beta booster.
#
def ProduceBE_Booster
{
  push(p);
  p=RandomCardSet("BE",((11,"C"),(3,"U"),(1,"R")));
  i=0;
  while(i < 11)
  {
    if(random(121) < 46)
      p[i]=random(cards{"BE"}{"L"});
    i=i+1;
  }
  while(i < 14)
  {
    if(random(121) < 26)
      p[i]=random(cards{"BE"}{"L"});
    i=i+1;
  }
  if(random(121) < 4)
    p[i]=random(cards{"BE"}{"L"});
  return(p);
  p=pop();
}

